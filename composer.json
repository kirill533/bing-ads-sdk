{
    "name": "bim/bing-ads-sdk",
    "description": "An alternate Bing Ads SDK",
    "keywords": ["bing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Davis",
            "homepage": "https://chrisguitarguy.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/AgencyPMG/bing-ads-php/issues"
    },
    "require": {
        "php": "^7.4",
        "ext-soap": "*",
        "league/oauth2-client": "~2.0",
        "guzzlehttp/psr7": "^1.0|^2.0",
        "psr/http-message": "~1.0",
        "psr/event-dispatcher": "^1.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.2",
        "wsdl2phpgenerator/wsdl2phpgenerator": "*",
        "symfony/filesystem": "~4.1",
        "psr/log": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "PMG\\BingAds\\": "src/main/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PMG\\BingAds\\": "test/",
            "PMG\\BingAds\\Generator\\": "src/generator/"
        }
    }
}
